{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "manage.py",
        "methods": ["GET", "POST"]
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "PiQum_Web.settings"
    }
  }